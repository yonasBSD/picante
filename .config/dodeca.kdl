content "docs/content"
output "docs/public"
link_check {
}
stable_assets {
}
code_execution {
    enabled #true
    fail_on_error #true
    timeout_secs 30
    cache_dir ".cache/code-execution"

    dependency "tokio" version="1.0"
    dependency "picante" version="0.1.0" path="crates/picante"
    dependency "facet" version="0.32.2"

    rust {
        command "cargo"
        args "run" "--quiet" "--release"
        extension "rs"
        prepare_code #true
        auto_imports "use std::sync::Arc;"
        show_output #true
    }
}
